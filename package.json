{
  "name": "tasklog",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "run-p lint:ts lint:css",
    "lint:fix": "run-p lint:ts:fix lint:css:fix",
    "lint:ts": "npx eslint src --ext \"ts,vue,js\"",
    "lint:ts:fix": "npx eslint src --ext \"ts,vue,js\" --fix",
    "lint:css": "stylelint \"src/**/*.vue\"",
    "lint:css:fix": "stylelint \"src/**/*.vue\" --fix",
    "test": "npm-run-all -p test:firebase test:ts",
    "test:firebase": "start-server-and-test \"npx firebase emulators:start\" http://localhost:4000 \"jest --testPathPattern='firebase.test.ts'\"",
    "test:ts": "jest --testPathIgnorePatterns='firebase.test.ts'"
  },
  "dependencies": {
    "firebase": "7.24.0",
    "fp-ts": "2.9.1",
    "tailwindcss": "1.9.6",
    "uuid": "8.3.1",
    "vue": "3.1.1",
    "vue-router": "4.0.8"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@firebase/testing": "0.20.11",
    "@types/jest": "26.0.16",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "@vue/compiler-sfc": "3.1.1",
    "@vue/eslint-config-typescript": "7.0.0",
    "babel-jest": "26.6.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.1.0",
    "firebase-tools": "8.16.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "sass": "1.29.0",
    "start-server-and-test": "1.11.6",
    "stylelint": "13.8.0",
    "stylelint-config": "0.1.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "typescript": "4.1.2",
    "vite": "1.0.0-rc.13"
  }
}
